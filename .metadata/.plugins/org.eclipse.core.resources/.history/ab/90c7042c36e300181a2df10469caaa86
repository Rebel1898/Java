package com.sls;

public class Helloprincipal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		String texto;
		String nombre="Sergio";
		texto="Hola ";
		String concat;
		int edad=20000;
		concat=texto+" "+nombre+" tienes "+edad+" años";
		double coste =23.45;
		float coste2=67.45f;//HAy que ponerle f
		
		double total= coste+coste2;
		
		
		System.out.println(concat);
		System.out.println(total);
		
	}

}
